# Database Configuration
DB_PATH=file:campuscash.db?cache=shared

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# SMTP Configuration (for email notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
